import http from "@/utils/request";
//分页查询区域管理
export function findArcSysSrea(params) {
  return http({
    method: "get",
    url: "/idlcore/findArcSysSrea",
    params,
  });
}

//查询区域
export function findAreaCode(params) {
  return http({
    method: "get",
    url: "/idlcore/findAreaCode",
    params: params,
  });
}

//新增修改区域
export function addAndUpdateAreaList(params) {
  return http({
    method: "post",
    url: "/idlcore/addAndUpdateAreaList",
    params: params,
  });
}

//批量删除区域
export function deleteAllAreaList(params) {
  return http({
    method: "post",
    url: "/idlcore/deleteAllAreaList",
    params: params,
  });
}
